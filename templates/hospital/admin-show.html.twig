{% extends 'base.html.twig' %}

{% block title %}Admin Hospital Listings{% endblock %}

{% block body %}

<div class="container">


<h1>Messages For:  {{ hospital.name }}</h1>

{% for message in messages %}

<p>Subject:  {{ message.subject }}</p>
<p>Email:  {{ message.email }}</p>
<p>Message:  {{ message.message }}</p>

    <hr/>

{% endfor %}

</div>


{% endblock %}


